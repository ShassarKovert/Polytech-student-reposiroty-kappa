#include <stdlib.h>
#include <stdio.h>
#include <math.h>
#define na 100

int main()
{
    system("chcp 1251");
    system("cls");
    int n, i = 1, i2 = 1, a[na];
    printf("Блок 1\nЭта программа выводит массив размера N с нечетными элементами\n\nВведите целое положительное число N: ");
    scanf_s("%d", &n);
    for (i = 0; i < n; i++, i2 += 2) 
        a[i] = i2;
    for (i = 0; i < n; i++) 
        printf("%d: %d\n", i, a[i]);
    return 0;
}
