#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int c;
	float a, b, d;
	printf("Блок 2\nЭта программа считает максимально возможное количество отрезков\nВведите число А: ");
	scanf_s("%f", &a);
	printf("Введите число В: ");
	scanf_s("%f", &b);
	d = a / b;
	c = (int)d;
	printf("Вот столько получилось: %d", c);
	return 0;
}
