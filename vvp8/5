#include <stdlib.h>
#include <stdio.h>
#include <math.h>

int main()
{
	system("chcp 1251");
	system("cls");
	int a, b, c, d, e;
	printf("Блок  5\nЭта программа ставит числа в трехзначном числе по-другому!\nВведите трехзначное число: ");
	scanf_s("%d", &a);
	e = a / 100;
	c = a % 100;
	b = c / 10;
	d = c % 10;
	printf("Получилось вот такое число: %d%d%d", b, d, e);
	return 0;
}
