#include <stdlib.h>
#include<stdio.h>
#include<math.h>

int main()
{
	system("chcp 1251");
	system("cls");
	int a, b, c, d;
	printf("Блок 4\nЭто программа считает количество квадратов, которые поместятся в прямоугольник без наложений\nВведите сторону А прямоугольника: ");
	scanf_s("%d", &a);
	printf("Введите сторону В прямоугольника: ");
	scanf_s("%d", &b); 
	printf("Введите сторону квадрата: ");
	scanf_s("%d", &c);
	d = (a / c) * (b / c);
	printf("%d - столько квадратов влезет в ваш прямоугольник", d);
	return 0;
}
