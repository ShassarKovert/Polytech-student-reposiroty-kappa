#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    system("chcp 1251");
    system("cls");
    int n, i;
    float o = 1;
    printf("Блок 2\nЭта программа находит произведение 1,1*1,2*1,3... И так далее\nВведите количество множителей: ");
    scanf_s("%d", &n);
     for (i = 0; i <= n; i++)
        o *= 1.0 + (float)i / 10;
    printf("rez = %f \n ", o);
    return 0;
}
